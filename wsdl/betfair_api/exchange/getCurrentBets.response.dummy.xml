<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:bfex="http://www.betfair.com/publicapi/v5/BFExchangeService/" xmlns:v5="http://www.betfair.com/publicapi/types/exchange/v5/">
   <soapenv:Header/>
   <soapenv:Body>
      <bfex:getCurrentBetsResponse>
         <bfex:Result>
            <header>
               <errorCode>?</errorCode>
               <minorErrorCode>?</minorErrorCode>
               <sessionToken>?</sessionToken>
               <timestamp>?</timestamp>
            </header>
            <bets>
               <!--Zero or more repetitions:-->
               <v5:Bet>
                  <asianLineId>?</asianLineId>
                  <avgPrice>?</avgPrice>
                  <betId>?</betId>
                  <betStatus>?</betStatus>
                  <betType>?</betType>
                  <betCategoryType>?</betCategoryType>
                  <betPersistenceType>?</betPersistenceType>
                  <cancelledDate>?</cancelledDate>
                  <lapsedDate>?</lapsedDate>
                  <marketId>?</marketId>
                  <marketName>?</marketName>
                  <fullMarketName>?</fullMarketName>
                  <marketType>?</marketType>
                  <marketTypeVariant>?</marketTypeVariant>
                  <matchedDate>?</matchedDate>
                  <matchedSize>?</matchedSize>
                  <matches>
                     <!--Zero or more repetitions:-->
                     <v5:Match>
                        <betStatus>?</betStatus>
                        <matchedDate>?</matchedDate>
                        <priceMatched>?</priceMatched>
                        <profitLoss>?</profitLoss>
                        <settledDate>?</settledDate>
                        <sizeMatched>?</sizeMatched>
                        <transactionId>?</transactionId>
                        <voidedDate>?</voidedDate>
                     </v5:Match>
                  </matches>
                  <placedDate>?</placedDate>
                  <price>?</price>
                  <bspLiability>?</bspLiability>
                  <profitAndLoss>?</profitAndLoss>
                  <selectionId>?</selectionId>
                  <selectionName>?</selectionName>
                  <settledDate>?</settledDate>
                  <remainingSize>?</remainingSize>
                  <requestedSize>?</requestedSize>
                  <voidedDate>?</voidedDate>
                  <handicap>?</handicap>
               </v5:Bet>
            </bets>
            <errorCode>?</errorCode>
            <minorErrorCode>?</minorErrorCode>
            <totalRecordCount>?</totalRecordCount>
         </bfex:Result>
      </bfex:getCurrentBetsResponse>
   </soapenv:Body>
</soapenv:Envelope>